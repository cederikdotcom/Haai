  "indexId": "interpersonal-complexity",
  "indexName": "Interpersonal Complexity Index",
  "version": "1.0.0",
  "description": "Measures the degree and type of human interaction required. Higher values = more complex social dynamics, lower values = less or no human interaction.",
  "lastUpdated": "2026-02-05",
  "scale": {
    "min": 0,
    "max": 4,
    "levels": [
      {"level": 0, "name": "None", "definition": "Solo task, no human interaction required."},
      {"level": 1, "name": "Informational", "definition": "Exchange information with humans (Q&A, status updates)."},
      {"level": 2, "name": "Coordinative", "definition": "Synchronize actions with humans (teamwork, scheduling)."},
      {"level": 3, "name": "Persuasive", "definition": "Influence human beliefs or behavior (sales, teaching, negotiation)."},
      {"level": 4, "name": "Relational", "definition": "Build trust, provide emotional support (caregiving, counseling, friendship)."}
    ]
  },
  "rationale": "Social complexity adds requirements for theory of mind, emotional intelligence, and interpersonal dynamics. Activities requiring relational depth are hardest for AI to replicate authentically.",
