  "classificationDecisionTree": {
    "description": "Algorithmic approach to resolve classification ambiguities",
    "steps": [
      {
        "step": 1,
        "question": "Is the primary output purely symbolic (numbers, code, formal logic)?",
        "ifYes": "Domain 1",
        "ifNo": "Continue to step 2"
      },
      {
        "step": 2,
        "question": "Does the activity require physical presence or manipulation?",
        "ifNo": {
          "subQuestion": "Is the output novel/creative or synthesis of existing info?",
          "ifNovel": "Domain 3",
          "ifSynthesis": "Domain 2"
        },
        "ifYes": "Continue to step 3"
      },
      {
        "step": 3,
        "question": "Is the primary purpose human wellbeing, trust, or emotional support?",
        "ifYes": "Domain 6",
        "ifNo": "Continue to step 4"
      },
      {
        "step": 4,
        "question": "Is the primary activity locomotion/navigation?",
        "ifYes": {
          "subQuestion": "For self or for work?",
          "ifSelf": "Domain 10",
          "ifWork": "Domain 9"
        },
        "ifNo": "Continue to step 5"
      },
      {
        "step": 5,
        "question": "Does the activity primarily involve human communication?",
        "ifYes": {
          "subQuestion": "Transactional/procedural or relationship-focused?",
          "ifTransactional": "Domain 5",
          "ifRelationship": "Domain 4"
        },
        "ifNo": "Continue to step 6"
      },
      {
        "step": 6,
        "question": "Is the environment controlled and predictable?",
        "ifYes": "Domain 7",
        "ifNo": {
          "subQuestion": "Requires acquired expertise/craft judgment?",
          "ifYes": "Domain 8",
          "ifNo": "Domain 10 (personal) or reconsider classification"
        }
      }
    ]
  },
